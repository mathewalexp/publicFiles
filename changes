$("#fileUploadDialog").dialog({
			modal: true, title: 'Create New Quiz', zIndex:10000, autoOpen: true,
			width: 'auto', 
			resizable: false,
			open: function(event, ui) {
				$("#activeFrom").datepicker();
				$("#activeTo").datepicker();
				
			    // Make the calendar icon clickable to open the datepicker
			    $(".calendar-icon").click(function() {
					$("#"+$(this).attr("element")).datepicker('show');
				});
			},
			buttons: {
				Upload: function() {
					if(callback !== null && callback !== undefined ) {
						callback();
					}
					
				},
				Close : function() {
					$(this).dialog("close");
				}
			},
			close: function(event, ui) {
				$(this).dialog("close");
			}
		});
