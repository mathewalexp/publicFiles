// Your XML string
let xmlString = `
<externalAttachments>
  <attachments>
    <fileName>abcd</fileName>
  </attachments>
  <attachments>
    <fileName>xyz</fileName>
  </attachments>
</externalAttachments>
`;

// Parse the XML string
let xmlDoc = $.parseXML(xmlString);

// Use jQuery to work with the parsed XML
let $xml = $(xmlDoc);

// Traverse through each <fileName> and log its text content
$xml.find("fileName").each(function() {
    console.log($(this).text());
});
